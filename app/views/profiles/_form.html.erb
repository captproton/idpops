<% form_for @profile do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :url %><br />
    <%= f.text_area :url %>
  </p>
  <p>
    <%= f.label :about %><br />
    <%= f.text_area :about %>
  </p>
  <p>
    <%= f.label :twitter_handle %><br />
    <%= f.text_field :twitter_handle %>
  </p>
  <p><%= f.submit "Submit" %></p>
<% end %>
